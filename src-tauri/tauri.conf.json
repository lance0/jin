{
  "productName": "Jin",
  "version": "1.0.0",
  "identifier": "com.jin.configwhisperer",
  "build": {
    "beforeDevCommand": "npm run dev:vite",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Jin - The Config Whisperer",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": "default-src 'self'; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; img-src 'self' data: asset: https://asset.localhost; font-src 'self' data:"
    }
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "publisher": "lance0",
    "category": "DeveloperTool",
    "shortDescription": "Config file management made easy",
    "longDescription": "Jin - The Config Whisperer helps you manage configuration files across your projects. Scan for .env, YAML, JSON, and TOML files, detect inconsistencies, and export templates—all locally.",
    "copyright": "Copyright © 2025 lance0. All rights reserved.",
    "licenseFile": "../LICENSE",
    "resources": [],
    "macOS": {
      "minimumSystemVersion": "10.13",
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "linux": {
      "deb": {
        "depends": []
      },
      "appimage": {
        "bundleMediaFramework": false
      }
    }
  },
  "plugins": {}
}
